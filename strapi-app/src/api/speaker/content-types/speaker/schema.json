{
  "kind": "collectionType",
  "collectionName": "speakers",
  "info": {
    "singularName": "speaker",
    "pluralName": "speakers",
    "displayName": "Speaker",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "speaker_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "speaker_id": {
      "type": "string"
    },
    "link": {
      "type": "string"
    },
    "debates": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::debate.debate",
      "inversedBy": "speakers"
    },
    "speeches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::speech.speech",
      "mappedBy": "speaker"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "text"
    },
    "gender": {
      "type": "string"
    },
    "place_of_birth": {
      "type": "string"
    },
    "educated_at": {
      "type": "text"
    },
    "website": {
      "type": "string"
    },
    "occupation": {
      "type": "text"
    },
    "languages": {
      "type": "text"
    },
    "date_of_birth": {
      "type": "string"
    },
    "date_of_death": {
      "type": "string"
    },
    "political_parties": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::political-party.political-party",
      "inversedBy": "speakers"
    }
  }
}
